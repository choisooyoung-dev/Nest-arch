<%- include('../component/header') %> 
<script src="/javascripts/user.js"></script>
  <body>
    <h2>User</h2>
      <ul class="userList">
        <% if (res.data) { %> 
          <% res.data.map(user => { %> 
            <li><%= user.name %>(<%= user.email%>)</li>
          <% }) %>
        <% } %>
      </ul>
    <hr>
    <input type="text" class="nameInput" placeholder="이름" value="">
    <input type="email" class="emailInput" placeholder="이메일" value="">
    <button id="add_btn">추가</button>
  </body>
</html>
